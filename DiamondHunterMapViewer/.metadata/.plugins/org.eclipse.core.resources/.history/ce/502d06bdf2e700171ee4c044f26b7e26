package com.neet.DiamondHunter.MapViewer;

@SuppressWarnings("serial")
public class ExceptionHandler extends Exception {
	
	  String excptxt;

	    ExceptionHandler(String excptxt){
	        this.excptxt = excptxt;
	    }

	    @Override
	    public String getMessage(){
	        return excptxt;
	    }
}
